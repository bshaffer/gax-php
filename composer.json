{
    "name": "google/gax",
    "type": "library",
    "description": "Google API Extensions for PHP",
    "keywords": ["google"],
    "homepage": "https://github.com/googleapis/gax-php",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "google/auth": "^0.11",
        "grpc/grpc": "*",
        "google/proto-client-php": "dev-proto3-changes"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.*",
        "phpdocumentor/phpdocumentor": "*"
    },
    "autoload": {
        "psr-4": {
          "Google\\GAX\\":"src/",
          "Google\\GAX\\UnitTests\\":"tests/"
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "/usr/local/google/home/michaelbausor/dev/clean_generation/php_proto3/proto-client-php"
    }]
}
